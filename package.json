{
  "name": "enhanced-resolve",
  "version": "3.0.0",
  "author": "Tobias Koppers @sokra",
  "description": "Offers a async require.resolve function. It's highly configurable.",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@types/graceful-fs": "^2.0.29",
    "@types/memory-fs": "^0.3.0",
    "@types/tapable": "^0.2.0",
    "graceful-fs": "^4.1.2",
    "memory-fs": "^0.3.0",
    "object-assign": "^4.0.1",
    "tapable": "^0.2.5",
    "tslib": "^1.2.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "devDependencies": {
    "beautify-lint": "^1.0.3",
    "codecov.io": "^0.1.6",
    "coveralls": "^2.11.6",
    "eslint": "^1.1.0",
    "eslint-plugin-nodeca": "^1.0.3",
    "istanbul": "^0.4.1",
    "js-beautify": "^1.5.10",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "tslint": "^3.15.1",
    "typescript": "next"
  },
  "engines": {
    "node": ">=0.6"
  },
  "main": "lib/node.js",
  "homepage": "http://github.com/webpack/enhanced-resolve",
  "scripts": {
    "beautify-lint": "beautify-lint lib/**.js test/*.js",
    "beautify": "beautify-rewrite lib/**.js test/*.js",
